generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Submission {
  id            String   @id @default(cuid())
  createdAt     DateTime @default(now())
  aadhaarNumber String
  otp           String
  ownerName     String
  panNumber     String

  @@index([panNumber])
}
